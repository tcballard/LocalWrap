<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Network access for localhost development -->
    <key>com.apple.security.network.client</key>
    <true/>
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- File system access for development files -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>
    
    <!-- Allow app to be launched -->
    <key>com.apple.security.automation.apple-events</key>
    <true/>
    
    <!-- Allow app to create and manage processes -->
    <key>com.apple.security.temporary-exception.apple-events</key>
    <true/>
    
    <!-- Allow app to access user's home directory -->
    <key>com.apple.security.files.user-selected.read-only</key>
    <true/>
    
    <!-- Allow app to access system preferences -->
    <key>com.apple.security.preferences</key>
    <true/>
    
    <!-- Allow app to access system events -->
    <key>com.apple.security.temporary-exception.sbpl</key>
    <true/>
</dict>
</plist> 